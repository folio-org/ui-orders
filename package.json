    "@folio/eslint-config-stripes": "^3.2.1",
    "@folio/stripes": "^1.0.0",
    "@folio/stripes-cli": "^1.6.0",
    "@folio/stripes-core": "^2.15.4",
    "babel-eslint": "^9.0.0",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.2.0",
    "eslint": "^5.6.0",
    "mocha": "^5.2.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-hot-loader": "^4.3.12",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "sinon": "^7.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.5",
    "moment": "^2.22.2",
    "prop-types": "^15.5.10",
    "query-string": "^5.0.0",
    "react-intl": "^2.4.0",
    "react-router-dom": "^4.1.1",
    "redux-form": "^7.3.0"
  },
  "peerDependencies": {
    "@folio/stripes": "^1.0.0",
    "react": "*"
